pipeline {
    agent any 
    stages {
        stage('Test') {
            steps {
                echo "test"
            }
        }
        stage('Build') {
            steps {
                sh 'docker build -t docker_coinbase .'
            }
        }
        stage("Start") {
            steps {
                sh 'docker run --rm -i -p 127.0.0.1:8083:8083 docker_coinbase'
            }
        }
    }
}